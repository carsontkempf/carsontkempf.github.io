{
  "name": "carsontkempf-github-io",
  "version": "1.0.0",
  "description": "Carson's personal website with Jekyll frontend and integrated code comprehension project",
  "dependencies": {
    "@auth0/auth0-spa-js": "^2.1.3",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "express-jwt": "^8.5.1",
    "express-oauth2-jwt-bearer": "^1.6.0",
    "googleapis": "^144.0.0",
    "google-auth-library": "^9.15.0",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.2.0",
    "netlify-cli": "^17.0.0"
  },
  "scripts": {
    "test": "jest --config tests/jest.config.js",
    "test:e2e": "jest --config tests/jest.config.js --bail",
    "test:code-comprehension": "jest --config tests/jest.config.js tests/code-comprehension",
    "test:spotify-apple": "jest --config tests/jest.config.js tests/spotify-apple",
    "test:stripe-integration": "jest --config tests/jest.config.js tests/stripe-integration",
    "test:authentication": "jest --config tests/jest.config.js tests/authentication",
    "test:connections-integration": "jest --config tests/jest.config.js tests/connections-integration",
    "serve:dev": "JEKYLL_ENV=production bundle exec jekyll serve --config _config.yml,_config-dev.yml",
    "serve:prod": "JEKYLL_ENV=production bundle exec jekyll serve --config _config.yml",
    "serve:netlify": "netlify dev",
    "build:dev": "JEKYLL_ENV=production bundle exec jekyll build --config _config.yml,_config-dev.yml",
    "build:prod": "JEKYLL_ENV=production bundle exec jekyll build --config _config.yml"
  },
  "devDependencies": {
    "cross-spawn": "^7.0.3",
    "jest": "^29.0.0",
    "puppeteer": "^24.9.0",
    "tree-kill": "^1.2.2"
  },
  "keywords": [
    "jekyll",
    "personal-website",
    "code-comprehension",
    "flask-backend",
    "ai-refactoring"
  ],
  "author": "Carson Kempf",
  "license": "MIT"
}
